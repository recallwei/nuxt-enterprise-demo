{
  "name": "nuxt-ts-starter-template",
  "description": "🍀 An opinionated Nuxt.js/TypeScript starter template.",
  "author": "Bruce Song <recall4056@gmail.com> (https://github.com/recallwei/)",
  "scripts": {
    "build": "nuxt build --dotenv .env.production",
    "dev": "nuxt dev --dotenv .env.development",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "npx only-allow pnpm && nuxt prepare",
    "cspell:check": "cspell \"**\"",
    "eslint:check": "eslint \"**/*.{ts,tsx,js,jsx,vue}\" --color",
    "eslint:fix": "pnpm eslint:check --fix",
    "prettier:check": "prettier --check --ignore-unknown .",
    "prettier:fix": "prettier --write --ignore-unknown .",
    "type:check": "pnpm nuxi typecheck",
    "prepare": "husky install"
  },
  "dependencies": {
    "@vueuse/motion": "^2.0.0",
    "pinia": "^2.1.6"
  },
  "devDependencies": {
    "@brucesong/eslint-config-vue": "^1.0.12",
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@iconify/json": "^2.2.108",
    "@nuxt/devtools": "^0.8.2",
    "@nuxt/image": "1.0.0-rc.1",
    "@pinia/nuxt": "^0.4.11",
    "@vueuse/core": "^10.4.1",
    "@vueuse/nuxt": "^10.4.1",
    "autoprefixer": "^10.4.15",
    "cspell": "^7.2.0",
    "eslint": "^8.48.0",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "nuxt": "^3.7.0",
    "postcss": "^8.4.29",
    "prettier": "^3.0.3",
    "sass": "^1.66.1",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.2.2",
    "unplugin-icons": "^0.16.6",
    "vue-tsc": "^1.8.8"
  },
  "packageManager": "pnpm@8.5.0",
  "engines": {
    "node": ">=18.16.1",
    "pnpm": ">=8.5.0"
  },
  "private": true,
  "license": "MIT"
}
